{
  "package": {
    "dependencies": {
      "@sentry/browser": "^6.14.1",
      "@testing-library/jest-dom": "^5.15.0",
      "@testing-library/react": "^12.1.2",
      "@testing-library/user-event": "^13.5.0",
      "@types/classnames": "^2.3.1",
      "@types/jest": "^27.0.2",
      "@types/react": "^17.0.34",
      "@types/react-dom": "^17.0.11",
      "@types/react-router-dom": "^5.3.2",
      "aidbox-react": "^1.3.3",
      "classnames": "^2.3.1",
      "@craco/craco": "^6.4.0",
      "craco-babel-loader": "^0.1.4",
      "node-sass": "^6.0.1",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-router-dom": "^5.3.0",
      "react-scripts": "^4.0.3",
      "ts-jest": "^27.0.7",
      "typescript": "^4.4.4"
    },
    "scripts": {
      "start": "craco start",
      "build": "craco build",
      "test": "react-scripts test --watchAll=false",
      "eject": "react-scripts eject",
      "typecheck": "tsc"
    },
    "jest": {
      "collectCoverageFrom": ["**/*.{ts,tsx}", "!**/node_modules/**", "!**/contrib/**"],
      "coverageReporters": ["text-summary", "html"],
      "transform": {
        "^.+\\.tsx?$": "ts-jest"
      },
      "transformIgnorePatterns": [
        "node_modules/(?!axios)$",
        "^.+\\.module\\.(css|sass|scss)$"
      ]
    }
  }
}
